import win32com.client as win32

class EmailService:
    def _init_(self):
        self.sender_email = "<insert email>"
        self.password = "<insert password>"

    def sendEmail(self, email, message):
        outlook = win32.Dispatch('outlook.application')
        mail = outlook.CreateItem(0)

        mail.To = email
        mail.Subject = "Do-Not-Reply-RP"
        mail.Body = "Thank you for deposit the lost item.'

        mail.Send()
        print("Send")

sendEmail=EmailService().sendEmail(<insert email>)
